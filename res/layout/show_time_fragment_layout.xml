<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res/com.danilov.TimeLApp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">



    <com.jeremyfeinstein.slidingmenu.lib.SlidingMenu
            android:id="@+id/sliding_menu_layout"
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            app:viewBehind="@layout/show_time_fragment_menu"
            app:viewAbove="@layout/show_time_fragment_content"
            app:behindWidth="240dp"
            app:touchModeAbove="fullscreen"
            app:mode="right"
            app:fadeEnabled="true"
            app:behindScrollScale="0.8"/>


</RelativeLayout>